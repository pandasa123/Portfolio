{
    "name": "sanket-panda-portfolio",
    "description": "Sanket Panda Portfolio",
    "version": "3.0.0",
    "author": "pandasa@umich.edu",
    "dependencies": {
        "classnames": "^2.2.6",
        "framer-motion": "^1.2.2",
        "gatsby": "2.13.21",
        "gatsby-image": "^2.2.6",
        "gatsby-plugin-manifest": "^2.2.3",
        "gatsby-plugin-offline": "^2.2.4",
        "gatsby-plugin-postcss": "2.1.2",
        "gatsby-plugin-purgecss": "4.0.0",
        "gatsby-plugin-react-helmet": "3.1.2",
        "gatsby-plugin-sharp": "^2.2.7",
        "gatsby-plugin-transition-link": "^1.12.4",
        "gatsby-source-filesystem": "^2.1.5",
        "gatsby-transformer-sharp": "^2.2.3",
        "graphql": "14.4.2",
        "gsap": "^2.1.3",
        "prop-types": "15.7.2",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-helmet": "5.2.1",
        "typeface-roboto": "0.0.54",
        "typeit": "^6.0.3"
    },
    "keywords": [
        "gatsby",
        "tailwind",
        "tailwindcss",
        "purgecss"
    ],
    "license": "MIT",
    "scripts": {
        "analyze:lint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts .",
        "analyze:prettier": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx,css,md,json}'",
        "analyze": "npm run analyze:lint && npm run analyze:prettier",
        "build": "gatsby build",
        "deploy": "npm run build && npm run serve",
        "fix:lint": "eslint --ext .jsx --ext .js --ext .tsx --ext .ts . --fix",
        "fix:prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json}",
        "fix": "npm run fix:lint && npm run fix:prettier",
        "start": "gatsby develop",
        "serve": "gatsby serve",
        "test": ""
    },
    "devDependencies": {
        "babel-eslint": "10.0.2",
        "eslint": "^6.0.1",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-config-react-app": "4.0.1",
        "eslint-plugin-flowtype": "3.11.1",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.2",
        "eslint-plugin-react-hooks": "^1.6.1",
        "prettier": "1.18.2",
        "tailwindcss": "1.0.5"
    },
    "publishConfig": {
        "access": "public"
    }
}
